    "@roxi/routify": "^2.18.11",
    "@tsconfig/svelte": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
